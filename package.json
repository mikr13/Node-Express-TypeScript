{
  "name": "boilerplate",
  "version": "0.0.0",
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "cross-env ENV_FILE=production node -r ./env -r module-alias/register ./dist/start.js",
    "start-dev": "nodemon --config \"./util/nodemon.json\"",
    "test": "nodemon --config \"./util/nodemon.test.json\""
  },
  "author": "Mihir Kumar",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiKr13/Node-Express-TypeScript.git"
  },
  "keywords": [
    "Node",
    "Express",
    "Typescript",
    "boilerplate"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MiKr13/Node-Express-TypeScript/issues"
  },
  "homepage": "https://github.com/MiKr13/Node-Express-TypeScript#readme",
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.3.2",
    "module-alias": "^2.2.1",
    "morgan": "^1.9.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.13",
    "@types/find": "^0.2.1",
    "@types/jasmine": "^3.8.2",
    "@types/jsonfile": "^5.0.0",
    "@types/morgan": "^1.7.36",
    "@types/node": "^12.20.18",
    "@types/supertest": "^2.0.8",
    "find": "^0.3.0",
    "fs-extra": "^8.1.0",
    "jasmine": "^3.4.0",
    "jsonfile": "^5.0.0",
    "nodemon": "^2.0.12",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.20.1",
    "typescript": "^3.9.10"
  }
}
